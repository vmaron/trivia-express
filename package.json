{
  "name": "trivia-express",
  "version": "1.0.0",
  "description": "A rest api and React client app for multiple choice trivia questions",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Slav Maron",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "concurrently": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nodemon": "^1.19.1"
  }
}
